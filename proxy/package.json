    "@babel/core": "^7.17.9",
    "@babel/node": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.31",
    "@types/proxy-addr": "^2.0.0",
    "babel-cli": "^6.26.0",
    "concurrently": "^7.1.0",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.15",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
